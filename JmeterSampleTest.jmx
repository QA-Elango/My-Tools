// Get the concatenated loan numbers and issue types
def desc = vars.get(&quot;Description_ALL&quot;)
def type = vars.get(&quot;IssueType_ALL&quot;)

def commaSeparatedDesc = desc.split(&quot;,&quot;)
def commaSeparatedType = type.split(&quot;,&quot;)

def filePath = &quot;E:/Downloads/ALL_CSV_FILES/Description_IssueType.csv&quot;
def fileWriter = new FileWriter(filePath, true);

for (int i = 0; i &lt; commaSeparatedDesc.length; i++) {
    // Concatenate desc and type for each entry
    String entry = commaSeparatedDesc[i] + &quot;,&quot; + commaSeparatedType[i]
    
    // Write the entry to the CSV file
    fileWriter.append(entry)

    // Start a new line after each set of loan number and issue type
    fileWriter.append(System.getProperty(&quot;line.separator&quot;))
}

// Log the concatenated loan numbers and issue types
log.info(&quot;Description_ALL: &quot; + desc)
log.info(&quot;IssueType_ALL: &quot; + type)
log.info(&quot;Combined entries: &quot; + (commaSeparatedDesc + commaSeparatedType).join(&apos;,&apos;))

// Close the FileWriter
fileWriter.close()
</stringProp>
