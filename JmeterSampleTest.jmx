            <stringProp name="script">import org.apache.commons.io.FileUtils
import java.io.FileWriter

// Get the concatenated loan numbers and issue types
def no = vars.get(&quot;issueNo_ALL&quot;)
def issue = vars.get(&quot;issue_ALL&quot;)

def commaSeparatedNo = no.split(&quot;,&quot;)
def commaSeparatedIssue = issue.split(&quot;,&quot;)

def filePath = &quot;E:/Downloads/ALL_CSV_FILES/IssueNo_IssueType.csv&quot;
def fileWriter = new FileWriter(filePath, true);

for (int i = 0; i &lt; commaSeparatedNo.length; i++) {
    // Concatenate desc and type for each entry
    String entry = commaSeparatedNo[i] + &quot;,&quot; + commaSeparatedIssue[i]
    
    // Write the entry to the CSV file
    fileWriter.append(entry)

    // Start a new line after each set of loan number and issue type
    fileWriter.append(System.getProperty(&quot;line.separator&quot;))
}

// Log the concatenated loan numbers and issue types
log.info(&quot;issueNo_ALL: &quot; + no)
log.info(&quot;issue_ALL: &quot; + issue)
log.info(&quot;Combined entries: &quot; + (commaSeparatedNo + commaSeparatedIssue).join(&apos;,&apos;))

// Close the FileWriter
fileWriter.close()
</stringProp>
