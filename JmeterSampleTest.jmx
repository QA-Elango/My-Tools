            <stringProp name="script">import org.apache.commons.io.FileUtils
import java.io.FileWriter

// Get the concatenated loan numbers, issue types, and statuses
def id = &quot;${Lndr_Id}&quot;
def list = vars.get(&quot;loanList_ALL&quot;)
def per = vars.get(&quot;amt_ALL&quot;)
def sta = vars.get(&quot;status_ALL&quot;)
log.info(id)


// Check if all variables are not null and have the same number of elements
if (id &amp;&amp; list &amp;&amp; per &amp;&amp; sta &amp;&amp; list.split(&quot;,&quot;).length == per.split(&quot;,&quot;).length &amp;&amp; per.split(&quot;,&quot;).length == sta.split(&quot;,&quot;).length) {
    def commaSeparatedlist = list.split(&quot;,&quot;)
    def commaSeparatedper = per.split(&quot;,&quot;)
    def commaSeparatedsta = sta.split(&quot;,&quot;)

    def filePath = &quot;E:/Downloads/ALL_CSV_FILES/LND_Id_ListNo_Amt_Status.csv&quot;
    def fileWriter = new FileWriter(filePath, true)

    try {
        for (int i = 0; i &lt; commaSeparatedlist.length; i++) {
            // Check if the status is equal to 2
            if (commaSeparatedsta[i] == &quot;2&quot;) {
                // Concatenate loan number, issue type, status, and lender ID for each entry
                String entry = id + &quot;,&quot; + commaSeparatedlist[i] + &quot;,&quot; + commaSeparatedper[i] + &quot;,&quot; + commaSeparatedsta[i]

                // Write the entry to the CSV file
                fileWriter.append(entry)

                // Start a new line after each set of loan number, issue type, status, and lender ID
                fileWriter.append(System.getProperty(&quot;line.separator&quot;))
            }
        }

        // Log the concatenated loan numbers, issue types, statuses, and lender ID
        log.info(&quot;Lndr_Id: &quot; + id)
        log.info(&quot;loanList_ALL: &quot; + list)
        log.info(&quot;amt_ALL: &quot; + per)
        log.info(&quot;status_ALL: &quot; + sta)
        log.info(&quot;Combined entries with Status 2: &quot; + (id + commaSeparatedlist + commaSeparatedper + commaSeparatedsta).join(&apos;,&apos;))
    } finally {
        // Close the FileWriter in a &apos;finally&apos; block to ensure it gets closed even if an exception occurs
        fileWriter.close()
    }
} else {
    log.error(&quot;Error: Variables are null or have different lengths.&quot;)
}
</stringProp>
