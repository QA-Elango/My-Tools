Swapping between two number  
 
class swap { 
  public  static void main (String[] args) {    
      int x = 20; 
      int y = 10; 
      System.out.println(" Before Swaping x and y "+ x +" "+y); 
      x=x+y; 
      y=x-y; 
      x=x-y; 
      System.out.println("After Swapping x and y " + x +" "+ y); 
   } 
 } 
 
