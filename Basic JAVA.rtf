Parameterised  Constructor 
 
class Check  { 
    String name; 
    int id; 
 
    public Check(int n, String s)  { 
        id = n; 
        name = s; 
     } 
 
    public void display()  { 
        System.out.println(id + " " + name); 
     } 
 
    public static void main(String[] args)  { 
        Check T1 = new Check(8, "Hello"); 
        Check T2 = new Check(9, "Hi"); 
 
        T1.display(); 
        T2.display(); 
     } 
 } 
 
 
